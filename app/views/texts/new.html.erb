<div class="row">
  <div class = "span6">
  <%= simple_form_for @text do |f| %>
    <%= f.input :title %>
    <%= f.input :content, input_html: { class: 'redactor', rows: '10' } %>
    <%= f.input :language %>
    <%= f.input :tags %>
    <%= f.input :share %>
    <%= f.button :submit %>
  <% end %>
</div>
</div>